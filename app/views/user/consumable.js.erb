var parent = $('#resource').parent();
var parent2 = $('#health').parent();
var parent3 = $('#consumable').parent();
$('#resource').remove();
$('#health').remove();
$('#consumable').remove();
parent.prepend("<%=j render 'resource' %>");
parent2.prepend("<div class=\"progress progress-danger progress-striped active\" id=\"health\"><div class=\"bar\" style=\"width:<%= x = current_user.robot.current_health.to_f / current_user.robot.max_health.to_f * 100.0 %>%;\" ><%= current_user.robot.current_health %></div><small class='pull-right'><%= current_user.robot.max_health %></small></div>");
parent3.prepend("<%=j render 'consumable' %>");