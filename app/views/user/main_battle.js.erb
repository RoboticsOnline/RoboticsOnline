var parent = $('#result').parent();
var parent2 = $('#validation').parent();
var parent3 = $('#experience').parent();
var parent4 = $('#resource').parent();
var parent5 = $('#health').parent();
$('#result').remove();
$('#validation').remove();
$('#experience').remove();
$('#resource').remove();
$('#health').remove();
parent.prepend("<%=j render 'result' %>");
parent3.prepend("<div class=\"progress progress-warning progress-striped active\" id=\"experience\"><div class=\"bar\" style=\"width:<%= x = current_user.robot.current_experience.to_f / current_user.robot.max_experience.to_f * 100.0 %>%;\"> <%= current_user.robot.current_experience%></div> <small class='pull-right'> <%= current_user.robot.max_experience%> </small></div>");
parent4.prepend("<%=j render 'resource' %>");
parent5.prepend("<div class=\"progress progress-danger progress-striped active\" id=\"health\"><div class=\"bar\" style=\"width:<%= x = current_user.robot.current_health.to_f / current_user.robot.max_health.to_f * 100.0 %>%;\" ><%= current_user.robot.current_health %></div><small class='pull-right'><%= current_user.robot.max_health %></small></div>");