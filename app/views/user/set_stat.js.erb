var parent = $('#stats').parent();
var parent2 = $('#unused_talent_points').parent();
var parent3 = $('#health').parent();
$('#stats').remove();
$('#unused_talent_points').remove();
$('#health').remove();
parent.prepend("<%=j render 'stats' %>");
parent2.prepend("<% if current_user.robot.unused_talent_points > 0 %> <span class=\"label label-warning\" id=\"unused_talent_points\"> +<%= current_user.robot.unused_talent_points%> </span> <% end %>");
parent3.prepend("<div class=\"progress progress-danger progress-striped active\" id=\"health\"><div class=\"bar\" style=\"width:<%= x = current_user.robot.current_health.to_f / current_user.robot.max_health.to_f * 100.0 %>%;\" ><%= current_user.robot.current_health %></div><small class='pull-right'><%= current_user.robot.max_health %></small></div>");